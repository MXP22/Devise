<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:ads="http://schemas.android.com/apk/res-auto"
	android:orientation="vertical"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:backgroundTint="@color/switch_thumb_normal_material_light"
	>
	<LinearLayout
		android:orientation="horizontal"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:id="@+id/linearLayoutAds1">
		<com.google.android.gms.ads.AdView
			ads:adUnitId="@string/Banner_Ads4"
			ads:adSize="250x50"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:id="@+id/adViewAA" />
	</LinearLayout>
	<LinearLayout
		android:orientation="horizontal"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:id="@+id/linearLayoutads2">
		<com.google.android.gms.ads.AdView
			ads:adUnitId="@string/Banner_Ads5"
			ads:adSize="250x50"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:id="@+id/adViewBB" />
	</LinearLayout>
	
		<LinearLayout
			android:orientation="horizontal"
			android:layout_width="match_parent"
			android:layout_height="match_parent"
			android:id="@+id/linearLayoutads2">
			<ListView
				android:layout_width="match_parent"
				android:layout_height="match_parent"
				android:id="@+id/listViewConvistisseurDevise" />
		</LinearLayout>
	
		
</LinearLayout>
